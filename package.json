{
  "name": "jstate",
  "version": "1.0.0",
  "description": "Really friendly Flux implementation",
  "main": "./dist/jstate.js",
  "files": [
    "dist",
    "example",
    "test"
  ],
  "directories": {
    "example": "example"
  },
  "scripts": {
    "build": "gulp",
    "dev": "gulp dev",
    "start": "browser-sync start -s ./example/ -f ./dist/ ./example/ --ss ./dist/ --port 3000",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha",
    "test:coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "version": "npm run build && git add -A",
    "postversion": "git push && git push --tags",
    "patch": "npm version minor -m \"Release version %s\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rannn505/jstate.git"
  },
  "author": "Ran Cohen <rannn505@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rannn505/jstate/issues"
  },
  "homepage": "https://github.com/rannn505/jstate#readme",
  "keywords": [
    "jstate",
    "state",
    "state management",
    "state js",
    "state javascript",
    "easy state"
  ],
  "dependencies": {
    "gulp-util": "^3.0.8"
  },
  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.13",
    "browserify": "^14.3.0",
    "chai": "^3.5.0",
    "connect-history-api-fallback": "^1.3.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^3.0.4",
    "gulp-header": "^1.8.8",
    "gulp-htmlmin": "^3.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^2.1.2",
    "mocha": "^3.2.0",
    "stringify": "^5.1.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
